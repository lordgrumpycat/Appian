SELECT `TASK_METRIC_ID`,    `PROCESS_DISPLAY_NAME`,    `PROCESS_MODEL_NAME`,    `PROCESS_GENERIC_NAME`,    `TASK_UUID`,    `TASK_PRIORITY_NAME`,    `TASK_DISPLAY_NAME`,    `TASK_NAME`,    `TASK_STATUS_NAME`,    `TASK_OWNER`,    `TASK_ASSIGNEES`,    `TASK_START_TIME`,    `TASK_ASSIGNMENT_TIME`,    `TASK_COMPLETION_TIME`,REPLACE    (    REPLACE        (`TASK_DESCRIPTION`, '\n', ' '),        '\r',        ' '),`TASK_DEADLINE`,`TASK_ON_TIME`,`TASK_OVERDUE`,`COMPLETION_DURATION`,`COMPLETION_DURATION_MINS`,`NET_COMPLETION_DURATION`,`NET_COMPLETION_DURATION_MINS`,`LAG_DURATION`,`LAG_DURATION_MINS`,`NET_LAG_DURATION`,`NET_LAG_DURATION_MINS`,`WORK_DURATION`,`WORK_DURATION_MINS`,`NET_WORK_DURATION`,`NET_WORK_DURATION_MINS`,`TEAM_NAME`,`CREATED_AT`,`COMPLETION_DURATION_BAND`
FROM   `XXXXXXXXX`
WHERE  `TASK_STATUS_NAME` 
LIKE '%Completed%' 
AND DATE(`TASK_COMPLETION_TIME`) = '2024-XX-XX';